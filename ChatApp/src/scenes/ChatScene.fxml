    <TextArea fx:id="conversationTextArea" editable="false" layoutX="235.0" layoutY="114.0" prefHeight="396.0" prefWidth="551.0" text="" wrapText="true" />
    <TextArea id="sendMessageTextField" fx:id="sendMessageTextArea" layoutX="235.0" layoutY="519.0" prefHeight="67.0" prefWidth="494.0" text="" wrapText="true" />
