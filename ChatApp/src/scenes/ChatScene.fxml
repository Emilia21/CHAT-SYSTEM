    <ListView fx:id="onlineUsersListView" layoutX="14.0" layoutY="114.0" onMouseClicked="#loadConversationOnUserClicked" prefHeight="472.0" prefWidth="200.0" />
    <Label layoutX="14.0" layoutY="90.0" prefWidth="73.0" text="Online users" />
